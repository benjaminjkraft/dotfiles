Include ~/.ssh/google_compute_config

Host github.com
  ControlMaster no
  ControlPath none
  ControlPersist no
  VisualHostKey no

Host utility
  HostName utility.notion.so
  ForwardAgent yes

Host lu
  HostName diogenes.learningu.org
  User benkraft

Host *
  ControlMaster auto
  ControlPath /tmp/ssh_mux_%h_%p_%r
  ControlPersist 12h
  ForwardX11 no
  VisualHostKey yes
  IdentityFile ~/.ssh/id_ed25519
  IdentityFile ~/.ssh/id_rsa
