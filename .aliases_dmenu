bashrc exec gvim "$HOME/.bashrc"
vimrc exec gvim "$HOME/.vimrc"
xmonad.hs exec gvim "$HOME/.xmonad/xmonad.hs"
aliases exec gvim "$HOME/.aliases"
aliases_dmenu exec gvim "$HOME/.aliases_dmenu"
books exec gvim "$HOME/Dropbox/Misc/books.txt"
Dropbox term "$HOME/Dropbox"
Documents term "$HOME/Documents"
Downloads term "$HOME/Downloads"
Pictures term "$HOME/Pictures"
/ term "/"
/media term "/media"
